<?xml version="1.0"?>
<agentcubes-splash-window title="Welcome to AgentCubes" margin="0" width="480" height="520" zoomable="false" resizable="false" minimizable="false">
  <column align="stretch" valign="stretch">>
    <agentcube-view name="cubes" full-scene-anti-aliasing="true" vflex="1">
      <agent-hyper-cube segments="8"/>
    </agentcube-view>
    <row minimize="vertical" align="center">
      <label text="AgentCubes" width="150" height="25" size="20.0"/>/>
    </row>
    <row minimize="vertical" align="center">
      <label text="Version" width="100" align="right" height="25" size="14.0"/>/>
      <label name="agentcubes version" text="" width="100" height="25" size="14.0"/>/>
    </row>
    <spacer height="20"/>
    <row minimize="vertical" align="left">
      <label text="Frame Rate: " size="9.0" width="120" align="right"/>
      <label name="frame rate" size="9.0"  text="0" width="40" />
      <label name="FPS" size="9.0" text="[FPS] HyperAgentCube x8 benchmark" width="300"/>
    </row>
    <row minimize="vertical" align="left">
       <label text="OpenGL Version: " size="9.0" width="120" align="right"/> 
       <label name="version" text="" size="9.0" width="120"/>
    </row>
    <row minimize="vertical" align="left">
       <label text="Vendor: " size="9.0" width="120" align="right"/> 
       <label name="vendor" text="0" size="9.0" width="120"/>
    </row>
    <row minimize="vertical" align="left">
       <label text="Renderer: " size="9.0" width="120" align="right"/> 
