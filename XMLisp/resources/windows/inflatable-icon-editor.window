<?xml version="1.0"?><column align="stretch" valign="distribute" padding="10">  <row name="toolbar">    <image-button-segment>      <image-button image="draw-button.png" action="draw-tool-action" tooltip="Draw Tool"/>      <image-button image="erase-button.png" action="erase-tool-action" tooltip="Erase Tool"/>      <choice-image-button tooltip="Selection Tool">        <image-choice image="select-rect-button.png" action="rect-tool-action"/>        <image-choice image="select-polygon-button.png" action="polygon-tool-action"/>        <image-choice image="select-ellipse-button.png" action="ellipse-tool-action"/>       </choice-image-button>      <image-button image="magic-wand-button.png" action="magic-wand-tool-action" tooltip="Magic Wand Tool"/>      <image-button image="eye-dropper-button.png" action="eye-dropper-tool-action" tooltip="Eyedropper Tool"/>      <image-button image="paint-bucket-button.png" action="paint-bucket-tool-action" tooltip="Paint Bucket Tool"/>    </image-button-segment>    <spacer width="12"/>    <choice-image-button tooltip="Selection Tool">      <image-choice image="mirror-none-button.png" action="mirror-none-action"/>      <image-choice image="mirror-horizontally-button.png" action="mirror-horizontally-action"/>      <image-choice image="mirror-vertically-button.png" action="mirror-vertically-action"/>      <image-choice image="mirror-both-button.png" action="mirror-both-action"/>     </choice-image-button>    <spacer width="12"/>    <color-well action="color-well-action" name="color-swatch"/>    <spacer width="12"/>    <label text="alpha" width="40"/>    <slider width="60" action="adjust-alpha-action"/>    <label text="100%" name="alpha-text"  width="30" align="right"/>  </row>  <row-of-squares>    <icon-editor name="icon-editor" img-height="32" img-width="32" action="change-icon-action"/>    <inflated-icon-editor name="model-editor"/>  </row-of-squares>  <row align="stretch" minimize="vertical" padding="10">    <column align="stretch" valign="top" size="100 120">      <row align="stretch">        <label width="65" text="Pressure"/>        <slider action="adjust-pressure-action" name="pressure_slider" value="500"/>        <label name="pressuretext" align="right" width="35" text="0.0"/>     </row>      <row align="stretch"><label width="65" text="Ceiling"/><slider action="adjust-ceiling-action" name="ceiling_slider" value="1000"/><label name="ceilingtext" align="right" width="35" text="1.0"/></row>      <row align="stretch"><label width="65" text="Noise"/><slider action="adjust-noise-action"/><label name="noise-text" align="right" width="35" text="0.0"/></row>      <row align="stretch"><label width="65" text="Smooth"/><slider action="adjust-smooth-action"/><label name="smooth-text" align="right" width="35" text="0.0"/></row>      <row align="stretch"><label width="65" text="z offset"/><slider action="adjust-z-offset-action"/><label name="z-offset-text" align="right" width="35" text="0.0"/></row>    </column>    <separator width="12" size="12 120"/>    <column align="stretch" valign="top" size="100 180">      <row>        <label text="surfaces" width="70" align="right"/>        <small-spacer/>        <pop-up width="150">          <pop-up-item text="front" action="front-surface-action"/>          <pop-up-item text="front and back" action="front-and-back-surface-action"/>          <pop-up-item text="front and back, connected" action="front-and-back-connected-surface-action"/>          <pop-up-item text="cube" action="cube-surface-action"/>        </pop-up>       </row>       <row align="stretch"><label width="70" text="distance" align="right"/><small-spacer/><slider action="adjust-distance-action"/><label name="distance-text" align="right" width="25" text="0.0"/></row>       <row><label text="upright" width="70" align="right"/><small-spacer/><check-box action="upright-action"/></row>       <row><label text="billboard" width="70" align="right"/><small-spacer/><check-box/></row>    </column>  </row>  <row align="right" valign="bottom" padding="20" size="50 50">    <button text="Flatten" width="70" action="edit-icon-flatten-action"/>     <spacer width="10"/>    <cancel-button action="edit-icon-cancel-action"/>    <button text="Apply" width="68" action="edit-icon-apply-action"/>    <ok-button action="edit-icon-ok-action"/>  </row></column>