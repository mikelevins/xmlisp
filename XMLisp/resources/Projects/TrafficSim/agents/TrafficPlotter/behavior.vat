<?xml version="1.0"?>
<behavior comments="Plot traffic in red">
  <method disclosed="true" comments="Plot traffic diffusion">
    <while-running/>
    <rule>
      <if/>
      <then>
        <plot-agents-attribute attribute="TRAFFIC" row2="19" col2="19" color="1.0 0.0 0.0 0.4" offset="-2" agent1="STREET" show-agent1="true" agent2="SEMAPHORE" show-agent2="true" agent3="CAR" show-agent3="true"/>
      </then>
    </rule>
  </method>
</behavior>