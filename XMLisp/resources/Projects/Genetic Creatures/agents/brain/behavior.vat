<?xml version="1.0"?>
<behavior comments="I am user controlled">
  <method disclosed="true">
    <while-running/>
    <rule>
      <if/>
      <then/>
    </rule>
  </method>
  <method disclosed="true">
    <on selector="Reset"/>
    <rule>
      <if>
        <see-a class-name="Gene">
          <direction drow="1" dcol="-1"/>
        </see-a>
      </if>
      <then>
        <move>
          <direction dcol="-1"/>
        </move>
        <message selector="Reset">
          <direction/>
        </message>
      </then>
    </rule>
  </method>
  <method disclosed="true" comments="Move @Inc Counts To The Right">
    <on selector="Inc"/>
    <rule>
      <if>
        <test expression1="@Inc" operator="&gt;" expression2="0"/>
      </if>
      <then>
        <move>
          <direction dcol="1"/>
        </move>
        <set attribute="@INC" value="@Inc - 1"/>
        <message selector="Inc">
          <direction/>
        </message>
      </then>
    </rule>
  </method>
</behavior>