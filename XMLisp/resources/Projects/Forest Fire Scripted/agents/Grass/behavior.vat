      <if>
        <percent-chance expression="@tree_density"/>
      </if>
      <then>
        <new shape="TREE Tree">
          <direction/>
        </new>
        <erase>
          <direction/>
        </erase>
      </then>
    </rule>
    <rule>
      <if/>
      <then>
        <new shape="BUSH Bush">
          <direction/>
        </new>
        <erase>
          <direction/>
        </erase>
      </then>
    </rule>
  </method>
</behavior>